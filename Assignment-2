#include<iostream>
using namespace std;
class solution{
    public:
    void MoveZeros(int n,int arr[])
    {
        int index = 0;
        for(int i=0;i<n;i++)
        {
            if(arr[i]!=0)
            {
                arr[index]=arr[i];
                index++;
            }
        }

        for(int i =index;i<n;i++)
        {
            arr[i]=0;
        }
        
        for(int i=0;i<n;i++)
        {
            cout<<arr[i];
        }
    }
};

int main()
{
    solution s;
    int n;
    cout << "Enter size of array: ";
    cin >> n;

    int arr[n];
    cout << "Enter array elements: ";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

   
    s.MoveZeros(n, arr);
    return 0;
}
